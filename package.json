  "description": "Customizable DBSCAN clustering for arbitrary datasets",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdxOo/dbscan.git"
  },
  "keywords": [
    "cluster",
    "clustering",
    "DBSCAN",
    "density",
    "density-based-clustering",
    "statistics"
  ],
  "bugs": {
